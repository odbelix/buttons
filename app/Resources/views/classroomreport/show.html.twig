{% extends 'base.html.twig' %}

{% block body %}
    <h1>ClassroomReport</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ classroomReport.id }}</td>
            </tr>
            <tr>
                <th>Report_type_id</th>
                <td>{{ classroomReport.reporttypeid }}</td>
            </tr>
            <tr>
                <th>Classroom_id</th>
